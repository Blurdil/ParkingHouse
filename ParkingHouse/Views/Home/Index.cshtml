@model ParkingHouse.Models.ViewModels.Home.HomeIndexViewModel

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <div class="col-md-4">
        Buldozer Garage!
    </div>
</div>

<div class="row">
    <div class="col-md-4">
        Lediga Platser: @Html.DisplayFor(m => m.FreeLots)<br />
        Totalt antal platser: @Html.DisplayFor(m => m.NumberOfLots)

        <input type="button" value="Checka in bil" onclick="CheckIn()"/>
    </div>
    <div class="col-md-4">
        <h2>Get more libraries</h2>
        <p>NuGet is a free Visual Studio extension that makes it easy to add, remove, and update libraries and tools in Visual Studio projects.</p>
        <p><a class="btn btn-default" href="http://go.microsoft.com/fwlink/?LinkId=301866">Learn more &raquo;</a></p>
    </div>
    <div class="col-md-4">
        Lista på nästa 5 parkeringstider som går ut <br />
        @foreach (var item in Model.Vehicles)
        {
            <ul>
                <li>
                    
                    @Html.DisplayFor(m => item.ParkingTimeStop)
                </li>
            </ul>
        }
 </div>
</div>